		f1.acelerar();
		f1.acelerar();
		f1.acelerar();
		f1.acelerar();
		f1.acelerar();
		f1.acelerar();
		f1.acelerar();
		
		System.out.println(f1);
		
		Civic c1 = new Civic();
		
		c1.acelerar();
		c1.acelerar();
		c1.acelerar();
		c1.acelerar();
		c1.acelerar();
		c1.acelerar();
		
		System.out.println(c1);
	}
